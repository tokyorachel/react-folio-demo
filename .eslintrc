{
  parser: 'babel-eslint',
  plugins: ['import', 'jsx-a11y', 'react', 'react-hooks'],
  settings:{
    react: {
      version: 'detect'
    }
  },
  extends: [
    'plugin:import/recommended',
    'plugin:jsx-a11y/recommended',
    'plugin:react/recommended',
  ],
  rules: {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn"
  }
}